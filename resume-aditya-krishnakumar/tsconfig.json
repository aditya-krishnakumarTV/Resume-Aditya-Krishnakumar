{
  "compilerOptions": {
    "jsx": "react-jsx", 
    "allowJs": true,
    "checkJs": false,
    "isolatedModules": true,
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
